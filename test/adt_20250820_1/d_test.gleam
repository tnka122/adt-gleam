import adt_20250820_1/d
import gleam/float

pub fn d_test() {
  assert float.absolute_value(
      d.solve([2, 3], [[0, 0], [0, 1], [1, 2], [2, 0]]) -. 2.23606797749978969,
    )
    <. 1.0e-5
  assert float.absolute_value(
      d.solve([2], [[-100_000, -100_000], [100_000, 100_000]])
      -. 282_842.712474619009,
    )
    <. 1.0e-5

  assert float.absolute_value(
      d.solve([2, 6, 8], [
        [-17_683, 17_993],
        [93_038, 47_074],
        [58_079, -57_520],
        [-41_515, -89_802],
        [-72_739, 68_805],
        [24_324, -73_073],
        [71_049, 72_103],
        [47_863, 19_268],
      ])
      -. 130_379.280458974768,
    )
    <. 1.0e-5
}
