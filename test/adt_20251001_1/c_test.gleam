      [
        [128, 741, 239, 901],
        [1, 1],
        [314, 159, 26_535],
      ],
      [
        #(1, 1),
        #(2, 2),
        #(3, 3),
        #(1, 4),
      ],
    )
    == ["128", "1", "26535", "901"]
}
